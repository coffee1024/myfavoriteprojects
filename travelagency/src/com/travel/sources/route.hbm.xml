<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true" package="com.travel.entity">
    <class name="Route" table="route">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" not-null="true" />
        </property>
         <property name="beginTime" type="java.sql.Timestamp">
            <column name="begintime" />
        </property>
         <property name="endTime" type="java.sql.Timestamp">
            <column name="endtime" />
        </property>
         <property name="days" type="java.lang.Integer">
            <column name="days" />
        </property>
        <property name="keywords" type="java.lang.String">
            <column name="keywords" />
        </property>
        <property name="description" type="java.lang.String">
            <column name="description" />
        </property>
    </class>
</hibernate-mapping>
